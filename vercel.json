{
  "routes": [
    {
      "src": "^/api/(.*)$",
      "dest": "http://api.tepuy.net/$1"
    },
    {
      "src": "^/assets/(.*)$",
      "dest": "/assets/$1"
    },
    {
      "src": "^/(.*\\.(js|css|ico|png|jpg|svg|woff|woff2))$",
      "dest": "/$1"
    },
    {
      "src": "^(.*)$",
      "dest": "/index.html"
    }
  ]
}
